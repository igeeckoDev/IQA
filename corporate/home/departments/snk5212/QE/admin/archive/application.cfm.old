<CFAPPLICATION
	NAME="QE"
	setclientcookies="Yes"
	SESSIONMANAGEMENT="YES"
	SESSIONTIMEOUT=#CreateTimespan(0,8,0,0)#>

<cferror type = "Exception"
   template = "error_handler.cfm"
   mailTo = "Christopher.J.Nicastro@us.ul.com">

<cferror type = "Request"
   template = "error_handler.cfm"
   mailTo = "Christopher.J.Nicastro@us.ul.com">

<cfoutput>
<cfset CurYear = #Dateformat(now(), 'yyyy')#>
<cfset NextYear = #CurYear# + 1>
<cfset LastYear = #CurYear# - 1>

<cfset CurMonth = #Dateformat(now(), 'mm')#>
<cfset CurDate = #Dateformat(now(), 'mm/dd/yyyy')#>
<cfset CurTime = #Timeformat(now(), 'HH:mm:ss')#>
<cfset CurTimeDate = '#CurDate# #CurTime#'>
</cfoutput>

<cfinclude template="\global.cfm">
<cfset thisPath = ExpandPath("*.*")>
<cfset path = GetDirectoryFromPath(thisPath)>
<cfset basedir = replace(GetDirectoryFromPath(cgi.script_name), "admin\", "", "All")>
<cfset curdir = replace(GetDirectoryFromPath(cgi.script_name), "\", "", "All")>
<cfset carbackupdir = "../BackupDB/">
<cfset fullBaseDir = "../">
<cfset fullBaseIQADir = "../../IQA/">
<cfset fullBaseIQADir2 = "D:\webserver\corporate\home\depts\snk5212\IQA\">

<cfset Request.DSN = "CAR">
<cfset Request.IQA = "IQA">
<cfset Request.GCAR = "QE">
<cfset Request.Logs = "IQA_Log">

<cfset SiteTitle = "CAR Process Website">
<cfset KBSiteTitle = "IQA / CAR Process Website Knowledge Base">

<cfset header = "http://#CGI.Server_Name#/header/header.js">
<cfset CSS = "http://#CGI.Server_Name##curdir#css.css">

<cfif CGI.Server_Name is "usnbkiqas100p">
	<cfset ULNetCSS = "http://#CGI.Server_Name#/header/ulnetheader.css">
<cfelse>
	<cfset ULNetCSS = "http://#CGI.Server_Name#/header/ulnetheader.css">
</cfif>

<cfif NOT IsDefined("SESSION.Auth.IsLoggedIn")>
	<cfif cgi.path_info is NOT "/departments/snk5212/QE/admin/global_login.cfm" AND cgi.PATH_INFO is NOT "/departments/snk5212/QE/admin/global_login2.cfm">
   		<cflocation url="global_login.cfm" addtoken="No">
   </cfif>
</cfif>
